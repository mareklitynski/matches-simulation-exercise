{
  "sourceType": "module",
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2020"],
    "strictNullChecks": true,
    "resolveJsonModule": true
  }
}
